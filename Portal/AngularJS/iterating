<div>
  <div ng-init="names=['x','y','xy','x+y']">
    <li ng-repeat="name in names"><span ng-bind="name"></span></li>
  </div>
</div>

<div>
  <div ng-init="names=['Abhinandan','Kunu','AbhiKunu','Abhik']">
    <li ng-repeat="name in names"><span ng-bind="name"></span></li>
  </div>
</div>





<div>
  <div ng-init="people=[{name:'Abhi',title:'Parhi'},{name:'Abhina',title:'Parhi12'},{name:'Abh',title:'Parhi1'}]">
    <li ng-repeat="names in people"> {{names.name}} </li>
  </div>
</div>
